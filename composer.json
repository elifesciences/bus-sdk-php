{
    "name": "elife/bus-sdk",
    "description": "eLife Sciences bus SDK",
    "type": "library",
    "license": "MIT",
    "require": {
        "elife/api-sdk": "dev-puli",
        "elife/logging-sdk": "dev-master",
        "aws/aws-sdk-php": "^3.19",
        "jms/serializer": "^1.4",
        "doctrine/cache": "^1.6",
        "psr/log": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/console": "^3.2"
    },
    "suggest": {
        "symfony/console": "To use Command classes"
    },
    "autoload": {
        "psr-4": {
            "eLife\\Bus\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eLife\\Bus\\": "./src",
            "tests\\eLife\\Bus\\": "./tests/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "./bin/build-hook",
        "post-update-cmd": "./bin/build-hook"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:thewilkybarkid/api-sdk-php.git"
        }
    ]
}
