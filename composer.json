{
    "name": "elife/bus-sdk",
    "description": "eLife Sciences Bus SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\Bus\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\Bus\\": "./test/src"
        }
    },
    "repositories": [
      {"type": "vcs",
       "url": "https://github.com/elifesciences/api-sdk-php"},
      {"type": "vcs",
       "url": "https://github.com/elifesciences/logging-sdk-php"}
    ],
    "require": {
        "php": ">=7.0",
        "aws/aws-sdk-php": "^3.238",
        "doctrine/cache": "^1.6",
        "elife/api-sdk": "dev-relax-php-requirement",
        "elife/logging-sdk": "dev-relax-php-requirement",
        "jms/serializer": "^1.4",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/console": "^2.7 || ^3.2"
    },
    "conflict": {
        "sebastian/comparator": "<1.2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "symfony/console": "^2.7 || ^3.2, to use Command classes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
